.decl eq(x: number, y: number)
.decl fun(x: number, f: number, y:number)
eq(x, y) :- eq(y, x).
eq(x, z) :- eq(x, y), eq(y, z).
fun(x, f, z) :- fun(x, f, y), eq(z, y).
eq(x, y) :- fun(x, f, z), fun(y, f, z).
fun(0, 2, 1).
fun(3, 2, 4).
eq(1, 4).

.decl iseq(x: number, y: number)
.output iseq(filename="res.csv")
iseq(x, y) :- eq(x, y), x=0, y=3.

